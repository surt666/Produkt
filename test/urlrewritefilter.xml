<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
  <rule>
    <condition name="accept" operator="notequal">application/vnd.yousee.kasia2.produkt\+json</condition>
    <from>^((?!^/alive|^/tool|^/ping|^/config|^/public|^/autodoc|^/array).)*$</from>
    <set type="status">406</set>
    <to last="true">null</to>
    <!-- <to last="true">/public/emptybody.html</to> -->
  </rule>
</urlrewrite>